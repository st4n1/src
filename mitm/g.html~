						<script type="text/javascript">
			var locale = 'de';

			function setTypeOptions(sel) {
				switch (sel) {
					
						case "1":
						
		
			

						
							$("#total_size_text").show();
							$("#clientfilesize").html("4,3" + " KB");
							$("#buttonDownload").removeClass("ButtonGrey");
							$("#buttonDownload").addClass("ButtonYellow");
						
							$("browser").html("FireFox");

						
							$("#buttonDownload").attr("href","/de/flashplayer/download/?installer=Flash_Player_11.2_for_other_Linux_(YUM)_64-bit&standalone=1");
						
							$("#buttonDownload").removeClass("ButtonGrey");
							$("#buttonDownload").addClass("ButtonYellow");
							break;
					
						case "2":
						
		
			

						
							$("#total_size_text").show();
							$("#clientfilesize").html("6,90" + " MB");
						
							$("browser").html("Firefox");

						
							$("#buttonDownload").attr("href","/de/flashplayer/download/?installer=Flash_Player_11.2_for_other_Linux_(.tar.gz)_64-bit&standalone=1");
						
							$("#buttonDownload").removeClass("ButtonGrey");
							$("#buttonDownload").addClass("ButtonYellow");
							break;
					
						case "3":
						
		
			

						
							$("#total_size_text").show();
							$("#clientfilesize").html("6,88" + " MB");
						
							$("browser").html("Firefox");

						
							$("#buttonDownload").attr("href","/de/flashplayer/download/?installer=Flash_Player_11.2_for_other_Linux_(.rpm)_64-bit&standalone=1");
						
							$("#buttonDownload").removeClass("ButtonGrey");
							$("#buttonDownload").addClass("ButtonYellow");
							break;
					
						case "4":
						
		
			

						
							$("#total_size_text").show();
							$("#clientfilesize").html("3,83" + " MB");
						
							$("browser").html("Firefox");

						
							$("#buttonDownload").attr("href","apt:adobe-flashplugin?channel=$distro-partner");
						
							$("#buttonDownload").removeClass("ButtonGrey");
							$("#buttonDownload").addClass("ButtonYellow");
							break;
					
						default:
							$("#buttonDownload").attr("href","javascript:void(0)");
							$("#buttonDownload").removeClass("ButtonYellow");
							$("#buttonDownload").addClass("ButtonGrey");
							$("#total_size_text").hide();
							break;
				}
			}
        </script>
		
